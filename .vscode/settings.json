{
  "go.buildTags": "integration,scenario,sample",
  "gopls": {
    "build.buildFlags": ["-tags=integration,scenario,sample"],
    "build.env": {
      "GOFLAGS": "-tags=integration,scenario,sample"
    },
    "analyses": {
      "unusedparams": true,
      "unusedwrite": true
    },
    "staticcheck": true,
    "gofumpt": true
  },
  "go.useLanguageServer": true,
  "go.lintOnSave": "package",
  "go.formatTool": "goimports",
  "go.alternateTools": {
    "go": "go"
  }
}
